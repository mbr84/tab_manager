{
  "name": "Garbage Collector",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Manages tabs and prevents tab proliferation.",
  "browser_action": {
    "default_title": "Garbage Collector",
    "default_popup": "popup.html",
    "default_title": "Manage Garbage Collection"

  },
  "homepage_url": "https://github.com/mbr84",
  "content_scripts": [{
      "js": [
        "jquery.js",
        "garbageCollector.js",
        "contentscript.js"],
      "style":["style.scss"],
      "matches": ["<all_urls>"]
    }],
  "chrome_url_overrides" : {
    "newtab": "linksIndex.html"
    },
  "background": {
    "scripts": ["garbageCollector.js"]
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "web_accessible_resources": [
    "preventFormLoss.js"
  ]
}
